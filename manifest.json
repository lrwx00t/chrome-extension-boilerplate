{
  "manifest_version": 2,
  "name": "Chrome Template",
  "short_name": "Chrome Template",
  "description": "Chrome Template.",
  "version": "0.1",
  "author": "author",
  "permissions": [
    "notifications",
    "background",
    "storage",
    "alarms",
    "contextMenus"
  ],
  "options_ui": {
    "open_in_tab": true,
    "page": "options/options.html"
  },
  "browser_action": {
    "default_popup": "app.html"
  }
}
